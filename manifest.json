{
  "manifest_version": 3,
  "name": "%name%",
  "version": "%version%",
  "description": "%description%",
  "permissions": ["scripting"],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.iife.js"],
    "run_at": "document_end"
  }],
  "background": {
    "service_worker": "background.iife.js"
  },
  "action": {
    "default_title": "%name%",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "768": "icon.png"
  }
}
